<ol class="trait-list">
	{{#each choices as |choice key|}}
		<li>
			<label {{~#if choice.wildcard}} class="wildcard"{{/if}}>
				<input type="checkbox" name="{{#if ../prefix}}{{../prefix}}.{{/if}}{{key}}"
					{{checked choice.chosen}} {{disabled choice.disabled}}>
				{{choice.label}}
			</label>
			{{#if choice.children}}
				{{> "blackFlag.trait-list" choices=choice.children prefix=../prefix}}
			{{/if}}
		</li>
	{{/each}}
</ol>
