<form autocomplete="off">
	<fieldset>
		<legend>{{ localize "BF.Senses.Label" }}</legend>
		{{#each types}}
			<label>
				<span>{{ label }}</span>
				<input type="text" name="types.{{ @key }}" value="{{ value }}" placeholder="{{ placeholder }}">
			</label>
		{{/each}}
	</fieldset>
	<fieldset>
		<legend>{{ localize "BF.Senses.Tag.Label" }}</legend>
		{{> "blackFlag.trait-list" choices=tagOptions prefix="tags" }}
	</fieldset>
</form>
